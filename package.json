    "boostgpt",
    "chatbot",
    "omnichannel",
    "router",
    "discord",
    "telegram",
    "slack",
    "whatsapp",
    "crisp",
    "AI",
    "chatgpt",
    "gpt",
    "anthropic",
    "xai",
    "gemini",
    "multi-channel",
    "conversational-ai"
  ],
  "author": "David Oti <hello@davidoti.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostgpt/boostgpt-router.git"
  },
  "bugs": {
    "url": "https://github.com/boostgpt/boostgpt-router/issues"
  },
  "homepage": "https://github.com/boostgpt/boostgpt-router#readme",
  "dependencies": {
    "boostgpt": "^2.0.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.11.0",
