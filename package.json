    "crisp-api": "^7.4.1",
    "whatsapp-web.js": "^1.19.5",
    "qrcode-terminal": "^0.12.0",
    "@slack/bolt": "^3.13.0"
  },
  "peerDependencies": {
    "boostgpt": "^1.1.6"
  },
  "devDependencies": {
    "esbuild": "^0.19.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "src/",
    "dist/",
    "utils/",
    "examples/",
    "README.md",
    "LICENSE",
    ".env.example"
