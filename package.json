{
  "name": "@boostgpt/router",
  "version": "1.0.0",
  "description": "Omnichannel AI message router - Deploy to Discord, Telegram, Slack, WhatsApp & Crisp with one codebase",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    }
